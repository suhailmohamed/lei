{
  "homepage": "http://suhailmohamed.github.io/lei",
  "name": "lei",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-scripts": "2.1.5",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "browser-sync": "^2.23.6",
    "node-sass-chokidar": "^1.3.4",
    "react-bootstrap": "^1.0.0-beta.5",
    "sass": "^1.17.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "deploy": "gh-pages -d build",
    "build": "yarn public-css && react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "public-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o public/",
    "public-sync": "yarn && browser-sync start -s --serveStatic 'public' --files 'public' --port 4000"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
